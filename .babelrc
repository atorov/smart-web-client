{
    "env": {
        "production": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "useBuiltIns": "entry",
                        "corejs": {
                            "version": 3,
                            "proposals": true
                        }
                    }
                ]
            ]
        },
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "modules": false,
                        "useBuiltIns": "entry",
                        "corejs": {
                            "version": 3,
                            "proposals": true
                        }
                    }
                ]
            ]
        },
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "useBuiltIns": "entry",
                        "corejs": {
                            "version": 3,
                            "proposals": true
                        }
                    }
                ]
            ]
        }
    },
    "ignore": [
        "__storage__",
        "build",
        "dist",
        "docs",
        "externals",
        "node_modules"
    ]
}
